{
    "name": "becklyn/ddd-symfony-bridge",
    "description": "Symfony bridge for use with becklyn/ddd-core and becklyn/ddd-doctrine-bridge",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Hautzinger",
            "email": "ch@becklyn.com"
        },
        {
            "name": "Marko Vujnovic",
            "email": "mv@becklyn.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "becklyn/ddd-core": "^3.1",
        "becklyn/ddd-doctrine-bridge": "^3.0",
        "doctrine/doctrine-bundle": "^1.10||^2.0",
        "sensio/framework-extra-bundle": "^5.0||^6.0",
        "simple-bus/message-bus": "^6.0",
        "simple-bus/symfony-bridge": "^6.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "doctrine/doctrine-migrations-bundle": "^3.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Ddd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Becklyn\\Ddd\\Tests\\": "tests/"
        }
    }
}
